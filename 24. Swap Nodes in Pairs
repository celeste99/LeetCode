class Solution {
    public ListNode swapPairs(ListNode head) {
        ListNode dummy = new ListNode(0);
        ListNode prev = dummy;
        prev.next = head;
        
        while (head != null && head.next != null) {
            ListNode next = head.next.next;
            prev.next = head.next;
            prev.next.next = head;
            head.next = next;
            prev = head;
            head = next;
        }
        
        return dummy.next;
    }
}
